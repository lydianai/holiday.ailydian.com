  "name": "travel-lydian-enterprise",
  "version": "2.0.0",
  "description": "Enterprise-grade AI-powered global tourism platform - Travel LyDian",
  "private": false,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "node scripts/prisma-generate-optional.js && next build",
    "build:vercel": "node scripts/prisma-generate-optional.js && next build",
    "start": "next start -p 3100",
    "postinstall": "node scripts/prisma-generate-optional.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:create-test-user": "ts-node scripts/create-test-user.ts",
    "lint": "next lint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "build:production": "prisma generate && next build",
    "cache:clear": "rm -rf .next && npm run build",
    "performance:test": "npm run build && npm run start &",
    "build:fast": "NEXT_TELEMETRY_DISABLED=1 next build",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3100 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3100 cypress:headless",
    "test:all": "npm run test:ci && npm run e2e:headless",
    "content:test": "ts-node scripts/content-generation/test-generation.ts",
    "content:cli": "ts-node scripts/content-generation/cli.ts",
    "content:generate": "ts-node scripts/content-generation/run-batch.ts",
    "content:generate:test": "BATCH_SIZE=10 ts-node scripts/content-generation/run-batch.ts",
    "content:generate:full": "BATCH_SIZE=1000 CONCURRENCY=15 ts-node scripts/content-generation/run-batch.ts"
  },
